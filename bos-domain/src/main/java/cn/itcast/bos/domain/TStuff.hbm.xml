<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.itcast.bos.domain.TStuff" table="t_stuff" schema="bos">
        <id name="id" column="id"/>
        <property name="name" column="name"/>
        <property name="telephone" column="telephone"/>
        <property name="haspda" column="haspda"/>
        <property name="daltag" column="daltag"/>
        <property name="standard" column="standard"/>
        <property name="station" column="station"/>
    </class>
</hibernate-mapping>